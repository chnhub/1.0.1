{

	"manifest_version": 2,
	"name": "demo",
	"version": "1.0.0",
	"description": "测试demo",
	
	"icons":
	{
		"16": "img/default.ico",
		"48": "img/default.ico",
		"128": "img/default.ico"
	},

	"background":
	{

		"page": "background.html"

	},

	"browser_action": 
	{
		"default_icon": "img/default.ico",
		"default_title": "wellcom",
		"default_popup": "popup.html"
	},


	"content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["js/jquery-1.8.3.min.js", "js/formatJson.js", "js/newStudent.js","js/content-script.js"],
			"run_at": "document_start"
		}
	],

	"permissions":
	[
		"contextMenus", 
		"tabs", 
		"notifications", 
		"webRequest", 
		"webRequestBlocking",
		"storage", 
		"http://*/*", 
		"https://*/*"

	],
	"web_accessible_resources": [
		"img/*"
	],
	"options_page": "options.html",
	"content_security_policy": "script-src 'self' https://cdn.staticfile.org https://unpkg.com https://cdn.jsdelivr.net; object-src 'self'"
	// https://unpkg.com https://cdn.staticfile.org https://cdn.jsdelivr.net   https://cdn.staticfile.orgobject-src

}